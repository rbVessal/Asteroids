package iceteroids {	import flash.display.MovieClip;		public class GameElement extends MovieClip	{		protected var vx: Number;		protected var vy: Number;		protected var vr: Number;		//Add a protected property called game:Game to the GameElement class		protected var game: Game;		public function GameElement(aGame:Game) 		{			// constructor code			trace(this + " created in GameElement");			//Initialize velocity x, velocity y, and velocity rotation			vx = 0;			vy = 0;			vr = 0;			this.game = aGame;			super();		}				public function move()		{			//move the game element			x += vx;			y += vy;						//rotate the game element			rotation += vr;						//wrap the game element on x			if(x < 0 - (width * 0.5))			{				x = stage.stageWidth + width * 0.5;			}						else if(x > stage.stageWidth + (width * 0.5))			{				x = 0 - (width * 0.5);			}						//wrap the game element on y			if(y < 0 - (height * 0.5))			{				y = stage.stageHeight + height * 0.5;			}			else if(y > stage.stageHeight + (height * 0.5))			{				y = 0 - (height * 0.5);			}		}	}	}